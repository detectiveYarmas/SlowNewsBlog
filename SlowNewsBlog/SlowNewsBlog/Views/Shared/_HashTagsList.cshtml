@model List<SlowNewsBlog.Models.Tables.HashTag>

<div class="row">
    <div class="col-xs-12">
        <div class="hashtag-list">
            <ul>
                @foreach (var tag in Model)
                {
                    <li>@tag.HashTagName</li>
                }
            </ul>
        </div>
    </div>
</div>